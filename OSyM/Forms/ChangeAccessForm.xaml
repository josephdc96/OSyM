<Window x:Class="OSyM.Forms.ChangeAccessForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OSyM.Forms"
        mc:Ignorable="d"
        Title="Change Access" Height="92.455" Width="228.091" ResizeMode="NoResize" Initialized="Window_Initialized">
    <Grid>
        <ComboBox x:Name="access" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="200">
            <ComboBoxItem Content="Employee"/>
            <ComboBoxItem Content="Department Manager"/>
            <ComboBoxItem Content="Building Manager"/>
        </ComboBox>
        <Button Content="OK" HorizontalAlignment="Left" Margin="135,36,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
